#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main()
{
	// 1단계
	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j <= i; j++)
		{
			printf("*");
		}
		printf("\n");
	}
	// 2단계

	for (int i = 0; i < 5; i++)
	{
		for (int j = 5; j > i; j--)
		{
			printf("*");
		}
		printf("\n");
	}

	return 0;

}
