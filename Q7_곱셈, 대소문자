#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main()
{
	int mul;
	
	

	for (int i = 1; i < 10; i++)
	{
		mul = 3 * i;
		printf("3X%d=%d\n", i, mul);

	}

	printf("\n");
	//A~Z
	for (int i = 'A'; i <= 'Z'; i++)
	{
		printf("%c", i);

	}
	printf("\n");
	for (int i = 'a'; i <= 'z'; i++)
	{
		printf("%c", i);
	}
	
	return 0;
}
